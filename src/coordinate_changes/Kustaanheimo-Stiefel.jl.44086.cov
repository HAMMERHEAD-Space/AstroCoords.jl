      547     if r_vec[1] >= 0
        0         u₁ = 0.0
        0         u₄ = √(0.5 * (r + r_vec[1]))
        0         u₂ = (r_vec[2]*u₁ + r_vec[3]*u₄) / (r + r_vec[1])
        0         u₃ = (r_vec[3]*u₁ - r_vec[2]*u₄) / (r + r_vec[1])
        -     else
      403         u₂ = 0.0
      547         u₃ = √(0.5 * (r - r_vec[1]))
      547         u₁ = (r_vec[2]*u₂ + r_vec[3]*u₃) / (r - r_vec[1])
      547         u₄ = (r_vec[3]*u₂ - r_vec[2]*u₃) / (r - r_vec[1])
        -     end
        - 
        -     # KS-Velocity
      559     u₅ = 0.5 * dot(SVector{3,RT}(u₁, u₂, u₃), v_vec)
      559     u₆ = 0.5 * dot(SVector{3,RT}(-u₂, u₁, u₄), v_vec)
